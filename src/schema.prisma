    datasource db {
      provider = "postgresql"
      url      = env("DATABASE_URL")
    }
    
    generator client {
      provider = "prisma-client-js"
    }
    
    model User {
      id    Int     @id @default(autoincrement())
      email String  @unique
      name  String?
      posts Post[]
    }
    
    model Post {
      id        Int      @id @default(autoincrement())
      title     String
      content   String?
      authorId  Int
      author    User     @relation(fields: [authorId], references: [id])
    }

